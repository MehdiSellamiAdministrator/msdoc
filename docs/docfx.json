{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "bin/**/*.dll"
          ]
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [ "**/*.{md,yml}" ],
        "exclude": [ "_site/**", "obj/**" ]
      }
    ],
    "resource": [
      {
        "files": [ 
          "**/images/**",
          "**/*.png",
          "**/*.gif",
          "**/*.jpg",
          "images/**"
        ],
        "exclude": [ "_site/**", "obj/**" ]
      }
    ],
    "postProcessors": [ "ExtractSearchIndex" ],
    "globalMetadata": {
      "_appTitle": "msDocs",
      "_appName": "msdocs",
      "_appLogoPath": "images/mslogo.svg",
      "_appFooter": "<span>Soutenu par le <a href=\"https://onetech-group.com\">Onetech Group</a>. Réalisé par <a href=\"https://github.com/MehdiSellamiAdministrator\">Mehdi Sellami</a></span>" 
    },
    "markdownEngineProperties": {
      "alerts": {
        "TODO": "alert alert-secondary"
      }
    },
    "sitemap": {
      "baseUrl": "https://mehdisellamiadministrator.github.io/msdoc/",
      "priority": 0.5,
      "changefreq": "daily"
    },
    "output": "_site",
    "template": [
      "default",
      "modern",
      "template"
    ]
  }
}